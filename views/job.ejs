<div class="container">
  <h1>Job ID <%= data.id %> <span class='badge'><%= data.name %></span></h1>
  <div class="container">
    <h1>Extracted Keyframes</h1>
    <div class="row well">
      <% for (var i=0; i<data.file.length; i++) { %>
        <div class="col-xs-6 col-md-3">
          <a href="<%= data.file[i] %>" class="thumbnail">
            <img src="<%= data.file[i] %>" alt="...">
          </a>
        </div>
      <% } %>
    </div>
  </div>
  <div class="container">
    <h1>Classifications</h1>
    <div class="row well">
      <% for (var i=0; i<data.file.length; i++) { %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <img src="<%= data.file[i] %>" alt="...">
            <div class="caption">
              <h3><%= data.dataKeyFrames[i] %></h3>
              <ul>
                <% for (var j=0; j<data.dataClassify[data.dataKeyFrames[i]].length;j++) { %>
                  <li><%= data.dataClassify[data.dataKeyFrames[i]][j] %></li>
                <% } %>
              </ul>
            </div>
          </div>
        </div>
      <% } %>
    </div>
  </div>
</div>
